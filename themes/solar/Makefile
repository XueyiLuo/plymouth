install-dist_themeDATA: $(dist_theme_DATA)
	@$(NORMAL_INSTALL)
	@list='$(dist_theme_DATA)'; test -n "$(themedir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(themedir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(themedir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(themedir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(themedir)" || exit $$?; \
	done

